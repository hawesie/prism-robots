#!/bin/sh

# Startup script for PRISM GUI (Linux/Solaris)

# PRISM home directory
PRISM_DIR=/home/bruno/devel_ws/src/policy_generator/mdp_plan_exec/prism-robots/prism

# Launch using main PRISM script
